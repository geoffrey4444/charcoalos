PROJECT_NAME = "CharcoalOS"
PROJECT_BRIEF = "A project to learn about operating system development"

OUTPUT_DIRECTORY = @DOXYGEN_OUTPUT_DIR@
GENERATE_HTML = YES
HTML_OUTPUT = html
GENERATE_LATEX = NO
GENERATE_MAN = NO
GENERATE_XML = NO

INPUT = @DOXYGEN_INPUT@
FILE_PATTERNS = *.h *.c *.s
RECURSIVE = YES
EXCLUDE_PATTERNS = @DOXYGEN_EXCLUDES@

OPTIMIZE_OUTPUT_FOR_C = YES
EXTRACT_ALL = YES
EXTRACT_STATIC = YES
EXTRACT_LOCAL_CLASSES = NO
JAVADOC_AUTOBRIEF = YES
QUIET = YES
WARN_IF_UNDOCUMENTED = NO
